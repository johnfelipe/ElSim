<!DOCTYPE html>
<html>
<% include ../partials/head.ejs %>
<% include ../partials/menu.ejs %>

<body>
<div class="container">
    <div class="card-deck-wrapper">
        <div class="card-deck">
            <% for(var i = 0, len = parties.length; i < len; i++) { %>
            <div class="card">
                <a href="<%= parties[i].description %>">
                    <img src="<%= parties[i].img %>" width="100%" class="card-img-top"/>
                </a>
            </div>
            <% } %>
        </div>

    </div>
</div>
<% include ../partials/footer.ejs %>
</body>
</html>
