<!DOCTYPE html>
<html>
<% include ../partials/head %>
<% include ../partials/menu %>

<body background="images/construction.jpg">
<div class="container-fluid">
    <div class="row">
        <div class="col-md-12">
            <div class="list-group">
                <a class="list-group-item active">
                    Datos
                </a>
                <a class="list-group-item list-group-item-action">
                    Author: <code><%= autor %></code>
                    Date: <code><%= fecha.slice(4,6) %>/<%= fecha.slice(0,4) %></code>
                    Province Code: <code><%= provincia %></code>

                </a>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-6">

            <div id="container"> </div>
        </div>
        <div class="col-md-6">
            <div id="container2"> </div>
        </div>
    </div>
</div>
<script src="https://code.highcharts.com/highcharts.js"></script>
<script src="https://code.highcharts.com/modules/exporting.js"></script>
<script type="text/javascript">
    jQuery.noConflict();
    var options = <%- JSON.stringify(options) %>,
            options2 = <%- JSON.stringify(options2) %>;
    (function($){ // encapsulate jQuery
        $(function () {
            $('#container').highcharts(options);
            $('#container2').highcharts(options2);
        });
    })(jQuery);
</script>
</body>
</html>
